{
  "name": "gelato-api",
  "version": "1.0.5",
  "description": "Gelato API Client in JavaScript/TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:strict": "eslint src --max-warnings 0",
    "lint:cache": "eslint src --cache",
    "test": "jest --config jest.config.cjs",
    "test:e2e": "jest --config jest.config.cjs --rootDir e2e",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denkan/gelato-api.git"
  },
  "keywords": [
    "gelato",
    "api",
    "gelato api client",
    "print api"
  ],
  "author": "Dennis Nystr√∂m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/denkan/gelato-api/issues"
  },
  "homepage": "https://github.com/denkan/gelato-api#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^26.6.0",
    "prettier": "^3.5.3",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "axios": "^0.24.0"
  }
}
