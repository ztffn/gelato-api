{
  "name": "gelato-api",
  "version": "1.0.5",
  "description": "Gelato API Client in JavaScript/TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest --config jest.config.cjs",
    "test:e2e": "jest --config jest.config.cjs --rootDir e2e",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denkan/gelato-api.git"
  },
  "keywords": [
    "gelato",
    "api",
    "gelato api client",
    "print api"
  ],
  "author": "Dennis Nystr√∂m",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/denkan/gelato-api/issues"
  },
  "homepage": "https://github.com/denkan/gelato-api#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.11.1",
    "jest": "^26.6.0",
    "prettier": "^2.3.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "axios": "^0.24.0"
  }
}
